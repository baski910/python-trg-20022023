   "source": [
    "# \\d to extract/match digits\n",
    "import re\n",
    "\n",
    "fh = open('numbers.txt','r')\n",
    "\n",
    "for line in fh.readlines():\n",
    "    sobj = re.search(r'.*#(\\d+)',line)\n",
    "    if sobj:\n",
    "        print(sobj.group(1))\n",
    "    else:\n",
    "        print(\"line has no digits\")\n",
    "        \n",
    "fh.close()\n",
    "    "
   ]
